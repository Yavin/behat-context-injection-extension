<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <service id="behat_context_injection_extension.argument_organizer"
                 class="Yavin\Behat\Extension\ContextInjection\ContextArgumentOrganizer">
            <argument/>
        </service>

        <service id="behat_context_injection_extension.argument_transformer"
                 class="Yavin\Behat\Extension\ContextInjection\ContextArgumentTransformer">
            <tag name="transformation.argument_transformer"/>
        </service>

    </services>
</container>
